				float smoothstepResult52 = smoothstep( ( _Range2 + ToonFade62 ) , ( _Range2 - ToonFade62 ) , saturate( NDotL22 ));
				float4 lerpResult32 = lerp( lerpResult26 , _ColorRamp2 , smoothstepResult52);
				float smoothstepResult57 = smoothstep( ( _Range3 + ToonFade62 ) , ( _Range3 - ToonFade62 ) , saturate( NDotL22 ));
				float4 lerpResult44 = lerp( lerpResult32 , _ColorRamp3 , smoothstepResult57);
				float smoothstepResult90 = smoothstep( ( _Range4 + ToonFade62 ) , ( _Range4 - ToonFade62 ) , saturate( NDotL22 ));
				float4 lerpResult92 = lerp( lerpResult44 , _ColorRamp4 , smoothstepResult90);
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 normalizeResult70 = normalize( ( _MainLightPosition.xyz + ase_worldViewDir ) );
				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
				float dotResult71 = dot( normalizeResult70 , ase_worldNormal );
				float smoothstepResult78 = smoothstep( ( _SpecularGlossiness - _SpecularSmoothness ) , ( _SpecularGlossiness + _SpecularSmoothness ) , pow( dotResult71 , _SpecularGlossiness ));
				float4 Specular81 = ( smoothstepResult78 * _SpecularColor );
				float dotResult117 = dot( ase_worldViewDir , ase_worldNormal );
				float smoothstepResult119 = smoothstep( _RimThreshold , _RimSmoothstep , max( 0.0 , dotResult117 ));
				float ase_lightAtten = 0;
