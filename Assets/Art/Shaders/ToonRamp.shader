				float4 rim126 = ( ( smoothstepResult119 * ase_lightAtten ) * _RimColor );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( lerpResult92 + Specular81 + rim126 ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
