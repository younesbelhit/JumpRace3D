				float4 appendResult12 = (float4(IN.ase_normal , 0.0));
				float4 break15 = mul( appendResult12, GetWorldToObjectMatrix() );
				float4 appendResult13 = (float4(break15.x , break15.y , break15.z , 0.0));
				float4 normalizeResult16 = normalize( appendResult13 );
				float4 normal17 = normalizeResult16;
				float dotResult20 = dot( normal17 , float4( _MainLightPosition.xyz , 0.0 ) );
				float NDotL22 = dotResult20;
				float smoothstepResult28 = smoothstep( ( _Range1 + ToonFade62 ) , ( _Range1 - ToonFade62 ) , saturate( NDotL22 ));
